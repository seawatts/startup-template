{
  "$schema": "https://turborepo.org/schema.json",
  "globalPassThroughEnv": [
    "NODE_ENV",
    "CI",
    "VERCEL",
    "VERCEL_ENV",
    "VERCEL_URL",
    "npm_lifecycle_event",
    "TURBO_TOKEN",
    "TURBO_TEAM"
  ],
  "tasks": {
    "//#clean": {
      "cache": false
    },
    "build": {
      "dependsOn": ["^build"],
      "outputs": [
        ".next/**",
        "!.next/cache/**",
        "next-env.d.ts",
        ".expo/**",
        ".output/**",
        ".vercel/output/**",
        "dist/**"
      ],
      "env": [
        "NEXT_PUBLIC_*",
        "DATABASE_URL",
        "POSTGRES_URL",
        "CLERK_SECRET_KEY",
        "POSTHOG_KEY",
        "OPENAI_API_KEY",
        "STRIPE_SECRET_KEY"
      ],
      "passThroughEnv": ["NPM_TOKEN"]
    },
    "@acme/web-app#build": {
      "env": [
        "NEXT_PUBLIC_*",
        "CLERK_SECRET_KEY",
        "CLERK_WEBHOOK_SECRET",
        "POSTGRES_URL",
        "POSTHOG_KEY",
        "OPENAI_API_KEY",
        "STRIPE_*"
      ]
    },
    "@acme/cli#build": {
      "env": ["NEXT_PUBLIC_*"],
      "passThroughEnv": ["APPLE_*", "MACOS_*", "NPM_TOKEN"]
    },
    "@acme/db#build": {
      "env": [
        "DATABASE_URL",
        "POSTGRES_*",
        "NEXT_PUBLIC_SUPABASE_*",
        "SUPABASE_PROJECT_ID"
      ],
      "passThroughEnv": ["SUPABASE_SERVICE_ROLE_KEY", "SUPABASE_JWT_SECRET"]
    },
    "clean": {
      "cache": false
    },
    "dev": {
      "cache": false,
      "persistent": true,
      "env": ["PORT"],
      "passThroughEnv": [
        "DATABASE_URL",
        "POSTGRES_*",
        "CLERK_*",
        "SUPABASE_*",
        "NEXT_PUBLIC_*",
        "OPENAI_API_KEY",
        "POSTHOG_*",
        "STRIPE_*",
        "KV_*",
        "REDIS_URL"
      ]
    },
    "format": {
      "dependsOn": ["^build"]
    },
    "gen-migration": {
      "cache": false,
      "interactive": true,
      "env": ["DATABASE_URL", "POSTGRES_URL"]
    },
    "migrate": {
      "cache": false,
      "interactive": true,
      "env": ["DATABASE_URL", "POSTGRES_URL"]
    },
    "push": {
      "cache": false,
      "interactive": true,
      "env": ["DATABASE_URL", "POSTGRES_URL"]
    },
    "seed": {
      "cache": false,
      "interactive": true,
      "env": ["DATABASE_URL", "POSTGRES_URL"],
      "passThroughEnv": ["CLERK_*", "SUPABASE_*"]
    },
    "studio": {
      "cache": false,
      "persistent": true,
      "passThroughEnv": ["DATABASE_URL", "POSTGRES_URL"]
    },
    "typecheck": {
      "outputs": ["node_modules/.cache/tsbuildinfo.json"]
    },
    "ui-add": {
      "cache": false,
      "interactive": true
    }
  },
  "ui": "tui"
}
