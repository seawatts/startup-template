{
  "catalog": {
    "@better-auth/expo": "1.4.7",
    "@supabase/supabase-js": "2.89.0",
    "@t3-oss/env-core": "0.13.10",
    "@t3-oss/env-nextjs": "0.13.10",
    "@tanstack/react-query": "5.90.12",
    "@trpc/client": "11.8.0",
    "@trpc/react-query": "11.8.0",
    "@trpc/server": "11.8.0",
    "@trpc/tanstack-react-query": "11.8.0",
    "@types/bun": "1.3.5",
    "@types/react": "19.2.7",
    "@types/react-dom": "19.2.3",
    "better-auth": "1.4.7",
    "date-fns": "4.1.0",
    "drizzle-orm": "0.45.1",
    "drizzle-zod": "0.8.3",
    "next": "16.1.0",
    "react": "19.2.3",
    "react-dom": "19.2.3",
    "stripe": "20.1.0",
    "superjson": "2.2.6",
    "tailwindcss": "4.1.18",
    "typescript": "5.9.3",
    "zod": "4.2.1",
    "zustand": "5.0.9"
  },
  "commitlint": {
    "extends": [
      "@seawatts/commitlint-config"
    ]
  },
  "devDependencies": {
    "@biomejs/biome": "2.3.10",
    "@changesets/changelog-github": "0.5.2",
    "@changesets/cli": "2.29.8",
    "@clack/prompts": "^0.11.0",
    "@commitlint/cli": "20.2.0",
    "@seawatts/commitlint-config": "workspace:*",
    "@testing-library/react": "16.3.1",
    "@types/bun": "1.3.5",
    "checkly": "6.9.7",
    "knip": "5.75.2",
    "lefthook": "2.0.12",
    "turbo": "^2.7.0",
    "typescript": "5.9.3",
    "vitest": "4.0.16",
    "zx": "^8.8.5"
  },
  "engines": {
    "node": ">=20.12.0"
  },
  "name": "@seawatts/root",
  "overrides": {
    "@types/vscode": "1.96.0"
  },
  "packageManager": "bun@1.2.23",
  "private": true,
  "scripts": {
    "build": "turbo run build",
    "bump-versions": "changeset version && bun install",
    "clean": "git clean -xdf node_modules .turbo",
    "clean:ws": "turbo run clean",
    "cleanup:all": "bun run scripts/cleanup-all-resources.ts",
    "cleanup:all:dry-run": "bun run scripts/cleanup-all-resources.ts --dry-run",
    "cleanup:orgs": "bun run scripts/cleanup-orgs.ts",
    "cleanup:orgs:dry-run": "bun run scripts/cleanup-orgs.ts --dry-run",
    "cleanup:webhooks": "bun run scripts/cleanup-webhooks.ts",
    "cleanup:webhooks:dry-run": "bun run scripts/cleanup-webhooks.ts --dry-run",
    "clone-project": "bun run scripts/clone-project.ts",
    "db:gen-migration": "turbo -F @seawatts/db gen-migration",
    "db:migrate": "turbo -F @seawatts/db migrate",
    "db:push": "turbo -F @seawatts/db push",
    "db:seed": "turbo -F @seawatts/db seed",
    "db:studio": "turbo -F @seawatts/db studio",
    "dev": "turbo watch dev dev:extension dev:webview",
    "dev:next": "turbo watch dev -F @seawatts/web-app...",
    "dev:vscode": "turbo watch dev dev:extension dev:webview -F seawatts-vscode... -F @seawatts/web-app...",
    "docs": "turbo run dev --filter=@seawatts/docs...",
    "ensure:user-resources": "bun run scripts/ensure-user-resources.ts",
    "ensure:user-resources:dry-run": "bun run scripts/ensure-user-resources.ts --dry-run",
    "format": "biome check && bun knip",
    "format:ci": "biome ci && bun knip",
    "format:fix": "biome check --write && bun knip --fix",
    "knip": "knip --include dependencies --cache",
    "lint:ws": "bunx sherif@latest",
    "postinstall": "bun lint:ws",
    "publish": "turbo run build --filter=./apps/cli... --filter=./packages/client... --filter=./apps/vscode-extension... && changeset publish",
    "scripts": "bun run scripts/run.ts",
    "sync-secrets": "bun run scripts/sync-secrets.ts",
    "test": "bun test",
    "test:integ": "turbo run test --filter=@seawatts/integ-test",
    "test:integ:ci": "turbo run test:ci --filter=@seawatts/integ-test",
    "test:integ:watch": "turbo run test:watch --filter=@seawatts/integ-test",
    "typecheck": "turbo run typecheck",
    "typecheck:ci": "turbo run build --filter @seawatts/stripe && turbo run typecheck",
    "ui-add": "turbo run ui-add",
    "version": "changeset version && bun biome check --write",
    "with-env": "infisical run --"
  },
  "trustedDependencies": [
    "@biomejs/biome",
    "@clerk/shared",
    "@scarf/scarf",
    "@sentry/cli",
    "@tailwindcss/oxide",
    "@vercel/speed-insights",
    "browser-tabs-lock",
    "bufferutil",
    "bun",
    "core-js",
    "electron",
    "esbuild",
    "keytar",
    "lefthook",
    "puppeteer",
    "sharp",
    "supabase",
    "yarn"
  ],
  "workspaces": {
    "packages": [
      "packages/*",
      "apps/*",
      "tooling/*",
      "docs"
    ]
  }
}
