{
  "author": "Acme, Inc.",
  "dependencies": {
    "@acme/analytics": "workspace:*",
    "@acme/api": "workspace:*",
    "@acme/ui": "workspace:*",
    "@clerk/chrome-extension": "2.5.25",
    "@clerk/elements": "0.23.58",
    "@plasmohq/messaging": "0.7.2",
    "@plasmohq/storage": "1.15.0",
    "@sentry/browser": "10.6.0",
    "@t3-oss/env-core": "0.13.8",
    "@tanstack/react-query": "5.85.5",
    "@trpc/client": "11.5.0",
    "@trpc/react": "9.27.4",
    "@trpc/react-query": "11.5.0",
    "@trpc/server": "11.5.0",
    "class-variance-authority": "0.7.1",
    "date-fns": "4.1.0",
    "lodash-es": "4.17.21",
    "pdfjs-dist": "5.4.54",
    "plasmo": "0.90.5",
    "pluralize": "8.0.0",
    "posthog-js": "1.260.3",
    "react": "19.1.1",
    "react-audio-voice-recorder": "2.2.0",
    "react-dom": "19.1.1",
    "react-error-boundary": "6.0.0",
    "react-pdf": "10.1.0",
    "react-router-dom": "7.8.2",
    "rooks": "9.0.1",
    "swr": "2.3.6",
    "tsx": "4.20.5",
    "zod": "3.25.76",
    "zustand": "5.0.8"
  },
  "description": "YC vibe-check: Use AI to auto-fill your application, get expert advice, and practice for interviews to secure funding",
  "devDependencies": {
    "@cove/tsconfig": "workspace:*",
    "@thedutchcoder/postcss-rem-to-px": "0.0.2",
    "@types/chrome": "0.1.4",
    "@types/lodash-es": "4.17.12",
    "@types/node": "24.3.0",
    "@types/pluralize": "0.0.33",
    "@types/react": "19.1.11",
    "@types/react-dom": "19.1.8",
    "autoprefixer": "10.4.21",
    "concurrently": "9.2.1",
    "cssnano": "7.1.1",
    "eslint": "9.34.0",
    "nodemon": "3.1.10",
    "postcss": "8.5.6",
    "postcss-focus-visible": "10.0.1",
    "postcss-import": "16.1.1",
    "prettier": "3.6.2",
    "rimraf": "6.0.1",
    "tailwindcss": "4.1.12",
    "ts-node": "10.9.2",
    "tsx": "4.20.5",
    "typescript": "5.9.2"
  },
  "displayName": "YC vibe-check: apply with ai",
  "manifest": {
    "host_permissions": [
      "https://apply.ycombinator.com/*",
      "http://localhost/*",
      "https://clerk.acme.ai/*",
      "https://immortal-koi-31.clerk.accounts.dev/*",
      "https://app.acme.ai/*",
      "https://*.s3.us-west-2.amazonaws.com/*"
    ],
    "key": "$CRX_KEY",
    "permissions": [
      "cookies",
      "storage"
    ],
    "web_accessible_resources": [
      {
        "extension_ids": [
          "$CRX_ID"
        ],
        "matches": [
          "<all_urls>"
        ],
        "resources": [
          "tabs/welcome.html",
          "tabs/account.html",
          "tabs/changelog.html"
        ]
      }
    ]
  },
  "name": "@cove/chrome-extension",
  "private": true,
  "scripts": {
    "build.skip": "plasmo build",
    "clean": "git clean -xdf .vite .turbo node_modules .plasmo build",
    "detect-obfuscation": "tsx ./scripts/detect-obfuscation/index.ts",
    "dev.skip": "concurrently \"nodemon --watch build/chrome-mv3-dev/_empty*.js --exec rimraf build/chrome-mv3-dev/_empty* \" \"plasmo dev\"",
    "package": "plasmo package",
    "postbuild": "rimraf build/chrome-mv3-prod/_empty*",
    "test": "vitest",
    "typecheck.skip": "tsc --noEmit"
  },
  "type": "module",
  "version": "1.1.0"
}
