{
  "activationEvents": [
    "onStartupFinished",
    "onUri:vscode",
    "onUri:vscode-insiders",
    "onUri:cursor",
    "onUri:windsurf"
  ],
  "bugs": {
    "url": "https://github.com/seawatts-sh/seawatts/issues"
  },
  "categories": [
    "Debuggers",
    "Testing",
    "Other"
  ],
  "contributes": {
    "authentication": [
      {
        "id": "seawatts",
        "label": "Seawatts"
      }
    ],
    "commands": [
      {
        "category": "Seawatts",
        "command": "seawatts.showWebhookEvents",
        "title": "Show Webhook Events"
      },
      {
        "category": "Seawatts",
        "command": "seawatts.addWebhookEvent",
        "title": "Add Webhook Event"
      },
      {
        "command": "seawatts.replayEvent",
        "icon": "$(play)",
        "title": "Replay Event"
      },
      {
        "command": "seawatts.viewEvent",
        "icon": "$(eye)",
        "title": "View Event"
      },
      {
        "command": "seawatts.viewRequest",
        "icon": "$(eye)",
        "title": "View Request"
      },
      {
        "command": "seawatts.replayRequest",
        "icon": "$(play)",
        "title": "Replay Request"
      },
      {
        "command": "seawatts.copyEvent",
        "icon": "$(copy)",
        "title": "Copy Event"
      },
      {
        "command": "seawatts.webhookEvents.refresh",
        "icon": "$(refresh)",
        "title": "Refresh"
      },
      {
        "command": "seawatts.openRequestDetails",
        "title": "Open Request Details"
      },
      {
        "category": "Seawatts",
        "command": "seawatts.quickPick",
        "icon": "$(list-selection)",
        "title": "Quick Pick Webhook Event"
      },
      {
        "command": "seawatts.webhookEvents.filter",
        "icon": "$(filter)",
        "title": "Filter Webhook Events"
      },
      {
        "category": "Seawatts",
        "command": "seawatts.focusOutput",
        "icon": "$(output)",
        "title": "Focus Output"
      },
      {
        "category": "Seawatts",
        "command": "seawatts.clearOutput",
        "icon": "$(clear-all)",
        "title": "Clear Output"
      },
      {
        "category": "Seawatts",
        "command": "seawatts.toggleOutput",
        "icon": "$(eye)",
        "title": "Toggle Output"
      },
      {
        "category": "Seawatts",
        "command": "seawatts.openSettings",
        "icon": "$(settings-gear)",
        "title": "Open Settings"
      },
      {
        "category": "Seawatts",
        "command": "seawatts.toggleAutoShowOutput",
        "icon": "$(eye)",
        "title": "Toggle Auto-Show Output"
      },
      {
        "category": "Seawatts",
        "command": "seawatts.toggleAutoClearEvents",
        "icon": "$(trash)",
        "title": "Toggle Auto-Clear Events"
      },
      {
        "category": "Seawatts",
        "command": "seawatts.toggleRequestDetailsView",
        "icon": "$(split-horizontal)",
        "title": "Toggle Request Details View"
      },
      {
        "category": "Seawatts",
        "command": "seawatts.signIn",
        "icon": "$(sign-in)",
        "title": "Sign in to Seawatts"
      },
      {
        "category": "Seawatts",
        "command": "seawatts.signOut",
        "icon": "$(sign-out)",
        "title": "Sign out of Seawatts"
      }
    ],
    "configuration": {
      "properties": {
        "seawatts.output.autoShow": {
          "default": true,
          "description": "Automatically show the output panel when new webhook events are received",
          "type": "boolean"
        },
        "seawatts.output.maxLines": {
          "default": 1000,
          "description": "Maximum number of lines to keep in the output panel",
          "type": "number"
        },
        "seawatts.requestDetails.defaultView": {
          "default": "formatted",
          "description": "Default view mode for request details",
          "enum": [
            "raw",
            "formatted"
          ],
          "type": "string"
        },
        "seawatts.webhookEvents.autoClear": {
          "default": false,
          "description": "Automatically clear old webhook events when the maximum history is reached",
          "type": "boolean"
        },
        "seawatts.webhookEvents.maxHistory": {
          "default": 100,
          "description": "Maximum number of webhook events to keep in history",
          "type": "number"
        }
      },
      "title": "Seawatts"
    },
    "menus": {
      "view/item/context": [
        {
          "command": "seawatts.copyEvent",
          "group": "inline",
          "when": "view == seawatts.webhookEvents && viewItem == webhookEvent"
        },
        {
          "command": "seawatts.viewEvent",
          "group": "inline",
          "when": "view == seawatts.webhookEvents && viewItem == webhookEvent"
        },
        {
          "command": "seawatts.replayEvent",
          "group": "inline",
          "when": "view == seawatts.webhookEvents && viewItem == webhookEvent"
        },
        {
          "command": "seawatts.viewRequest",
          "group": "inline",
          "when": "view == seawatts.webhookEvents && viewItem == webhookRequest"
        },
        {
          "command": "seawatts.replayRequest",
          "group": "inline",
          "when": "view == seawatts.webhookEvents && viewItem == webhookRequest"
        }
      ],
      "view/title": [
        {
          "command": "seawatts.quickPick",
          "group": "navigation",
          "when": "view == seawatts.webhookEvents"
        },
        {
          "command": "seawatts.webhookEvents.filter",
          "group": "navigation@1",
          "when": "view == seawatts.webhookEvents"
        }
      ]
    },
    "views": {
      "seawatts": [
        {
          "contextualTitle": "Webhook Events",
          "id": "seawatts.webhookEvents",
          "name": "Webhook Events"
        },
        {
          "contextualTitle": "Settings",
          "id": "seawatts.settings",
          "name": "Settings"
        },
        {
          "contextualTitle": "Request Details",
          "id": "seawatts.requestDetails",
          "name": "Request Details",
          "visibility": "hidden"
        }
      ]
    },
    "viewsContainers": {
      "activitybar": [
        {
          "icon": "$(radio-tower)",
          "id": "seawatts",
          "title": "Seawatts"
        }
      ]
    }
  },
  "dependencies": {
    "@seawatts/api": "workspace:*",
    "@seawatts/logger": "workspace:*",
    "@seawatts/ui": "workspace:*",
    "@seawatts/zustand": "workspace:*",
    "@t3-oss/env-core": "catalog:",
    "react": "catalog:",
    "react-dom": "catalog:"
  },
  "description": "Webhook Development",
  "devDependencies": {
    "@seawatts/tsconfig": "workspace:*",
    "@types/bun": "catalog:",
    "@types/node": "24.8.1",
    "@types/react": "catalog:",
    "@types/react-dom": "catalog:",
    "@types/vscode": "1.105.0",
    "@vitejs/plugin-react": "5.0.4",
    "@vscode/test-cli": "0.0.12",
    "@vscode/test-electron": "2.5.2",
    "concurrently": "9.2.1",
    "mocha": "11.7.4",
    "typescript": "catalog:",
    "vite": "7.1.10"
  },
  "displayName": "Seawatts - Webhook Development",
  "engines": {
    "vscode": ">=1.96.0"
  },
  "homepage": "https://github.com/seawatts-sh/seawatts#readme",
  "keywords": [
    "webhook",
    "development",
    "testing",
    "debugging"
  ],
  "main": "./dist/extension.js",
  "name": "seawatts-vscode",
  "private": true,
  "publisher": "seawatts",
  "repository": {
    "type": "git",
    "url": "https://github.com/seawatts-sh/seawatts"
  },
  "scripts": {
    "build": "bun run typecheck && bun run build:webview && bun run build:base --minify --production",
    "build:base": "bun build ./src/extension.ts --outfile ./dist/extension.js --target node --bundle --external vscode --format cjs",
    "build:debug": "bun run typecheck && bun run build:webview && bun run build:base --sourcemap=inline",
    "build:webview": "vite build",
    "clean": "git clean -xdf .turbo node_modules dist",
    "dev": "concurrently \"bun run dev:webview\" \"bun run dev:extension\"",
    "dev:extension": "NODE_ENV=development bun run build:base --watch --sourcemap=inline",
    "dev:webview": "NODE_ENV=development vite build --config vite.dev.config.ts",
    "package": "bun run typecheck && bun run build",
    "pretest": "bun run build:base",
    "test": "vscode-test",
    "typecheck": "tsc --noEmit --emitDeclarationOnly false",
    "vsce": "bunx vsce package",
    "vscode:prepublish": "bun run package"
  },
  "type": "commonjs",
  "version": "0.0.1"
}
